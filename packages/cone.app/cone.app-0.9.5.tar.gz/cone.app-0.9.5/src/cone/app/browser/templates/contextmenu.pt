<tal:block xmlns:tal="http://xml.zope.org/namespaces/tal"
           xmlns:ajax="http://namesspaces.bluedynamics.eu/ajax"
           omit-tag="True">
  
  <div class="contextmenu_wrapper"
       ajax:bind="contextactionschanged"
       ajax:action="contextmenu:.contextmenu_wrapper:replace">
    
    <div class="contextmenu">
    
      <tal:toolbars repeat="toolbar context.toolbars">
        <div tal:replace="structure toolbar(model, request)"></div>
      </tal:toolbars>
    
    </div>
  
  </div>
  
</tal:block>