<!-- htc-py generator configuration file -->

<htc>
	<website>
		<target>_top</target>

		<output>
			<filetype>html</filetype>
			<comments>yes</comments>
			<name>Chapter</name>
		</output>

		<design>
			<general>triple_framed_one_row</general>
			<grid>table</grid>
			<css>
				<file></file>
			</css>
		</design>
	</website>

	<frame>
		<id>1</id>
		<connections>
			<file></file>
			<routine></routine>
		</connections>

		<contents>index_linked</contents>

		<layout>
			<bgcolor>#EBF1F7</bgcolor>
			<bgimage></bgimage> 
			<framesize>35</framesize>
			<framescrolling>yes</framescrolling>
			<framesize_ip_as>percentage</framesize_ip_as>
			<index>current_chapter</index>
			<markedindex>current_headline</markedindex>
			<markedcolor>#FFFFFF</markedcolor>
			<indexnumber>numeric</indexnumber>
			<css>
				<layoutfile></layoutfile>
				<style></style>
			</css>
			<link>
				<layoutfile></layoutfile>
				<style></style>
			</link>
		</layout>
	</frame>

	<frame>
		<id>2</id>
		<connections>
			<file></file>
			<routine></routine>
		</connections>

		<contents>text_with_headline</contents>

		<layout>
			<bgcolor>#FFFFFF</bgcolor>
			<bgimage></bgimage> 
			<framesize>60</framesize>
			<framescrolling>yes</framescrolling>
			<framesize_ip_as>percentage</framesize_ip_as>
			<index></index>
			<markedindex></markedindex>
			<markedcolor></markedcolor>
			<indexnumber></indexnumber>
			<css>
				<layoutfile></layoutfile>
				<style></style>
			</css>
			<link>
				<layoutfile></layoutfile>
				<style></style>
			</link>
		</layout>
	</frame>

	<frame>
		<id>3</id>
		<connections>
			<file></file>
			<routine></routine>
		</connections>

		<contents>empty</contents>

		<layout>
			<bgcolor>#EBF1F7</bgcolor>
			<bgimage></bgimage> 
			<framesize>5</framesize>
			<framescrolling>yes</framescrolling>
			<framesize_ip_as>percentage</framesize_ip_as>
			<index></index>
			<markedindex></markedindex>
			<markedcolor></markedcolor>
			<indexnumber></indexnumber>
			<css>
				<layoutfile></layoutfile>
				<style></style>
			</css>
			<link>
				<layoutfile></layoutfile>
				<style></style>
			</link>
		</layout>
	</frame>

</htc>
