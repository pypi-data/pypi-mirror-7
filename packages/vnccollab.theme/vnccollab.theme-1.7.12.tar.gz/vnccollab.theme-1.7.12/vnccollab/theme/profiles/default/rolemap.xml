<?xml version="1.0"?>
<rolemap>
  <roles>
    <role name="VNCEditor"/>
  </roles>
  <permissions>
    <permission name="Access contents information"
                acquire="True">
      <role name="Contributor"/>
      <role name="Editor"/>
      <role name="Reader"/>
      <role name="VNCEditor"/>
    </permission>
    <permission name="CMFEditions: Access previous versions"
                acquire="True">
      <role name="Contributor"/>
      <role name="Editor"/>
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="Reviewer"/>
      <role name="VNCEditor"/>
    </permission>
    <permission name="CMFEditions: Apply version control"
                acquire="True">
      <role name="Contributor"/>
      <role name="Editor"/>
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="Reviewer"/>
      <role name="VNCEditor"/>
    </permission>
    <permission name="CMFEditions: Checkout to location"
                acquire="True">
      <role name="Editor"/>
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="Reviewer"/>
      <role name="VNCEditor"/>
    </permission>
    <permission name="CMFEditions: Revert to previous versions"
                acquire="True">
      <role name="Editor"/>
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="Reviewer"/>
      <role name="VNCEditor"/>
    </permission>
    <permission name="CMFEditions: Save new version"
                acquire="True">
      <role name="Contributor"/>
      <role name="Editor"/>
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="Reviewer"/>
      <role name="VNCEditor"/>
    </permission>
    <permission name="List folder contents"
                acquire="True">
      <role name="Contributor"/>
      <role name="Editor"/>
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="Reviewer"/>
      <role name="VNCEditor"/>
    </permission>
    <permission name="Modify portal content"
                acquire="True">
      <role name="Editor"/>
      <role name="Owner"/>
      <role name="VNCEditor"/>
    </permission>
    <permission name="Request review"
                acquire="True">
      <role name="Editor"/>
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="VNCEditor"/>
    </permission>
    <permission name="View"
                acquire="True">
      <role name="Contributor"/>
      <role name="Editor"/>
      <role name="Reader"/>
      <role name="VNCEditor"/>
    </permission>
    <permission name="iterate : Check in content"
                acquire="True">
      <role name="Editor"/>
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="VNCEditor"/>
    </permission>
    <permission name="iterate : Check out content"
                acquire="True">
      <role name="Editor"/>
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="VNCEditor"/>
    </permission>
    <permission name="WSAPI authenticated"
                acquire="True">
      <role name="Member"/>
      <role name="Editor"/>
      <role name="Manager"/>
      <role name="Owner"/>
      <role name="VNCEditor"/>
    </permission>

  </permissions>
</rolemap>
