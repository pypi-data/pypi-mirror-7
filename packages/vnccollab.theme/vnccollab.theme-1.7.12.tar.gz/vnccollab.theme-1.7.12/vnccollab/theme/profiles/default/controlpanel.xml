<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="vnccollab.theme"
    purge="False">

    <configlet
        title="WorldClock"
        action_id="worldclock"
        appId="vnccollab.theme.worldclock"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@worldclock-settings"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

    <configlet
        title="Anonymous Homepage"
        action_id="anonhomepage"
        appId="vnccollab.theme.anonhomepage"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@anonhomepage-settings"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
