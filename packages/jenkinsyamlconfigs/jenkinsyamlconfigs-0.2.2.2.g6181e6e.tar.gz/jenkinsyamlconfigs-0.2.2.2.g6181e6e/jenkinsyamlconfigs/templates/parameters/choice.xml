<hudson.model.ChoiceParameterDefinition>
  <name>{{ parameter.label }}</name>
  <description>{{ parameter.description }}</description>
  <choices class="java.util.Arrays$ArrayList">
    <a class="string-array">
      {% for value in parameter.value %}
      <string>{{ value }}</string>
      {% endfor %}
    </a>
  </choices>
</hudson.model.ChoiceParameterDefinition>