<scm class="hudson.plugins.git.GitSCM">
  <configVersion>2</configVersion>
  <userRemoteConfigs>
    <hudson.plugins.git.UserRemoteConfig>
      <name>{{ scm.repo_name or 'origin' }}</name>
      <refspec>{{ scm.repo_refspec }}</refspec>
      <url>{{ scm.repo_url }}</url>
    </hudson.plugins.git.UserRemoteConfig>
  </userRemoteConfigs>
  <branches>
  {% for branch in scm.branches %}
    <hudson.plugins.git.BranchSpec>
        <name>{{ branch }}</name>
    </hudson.plugins.git.BranchSpec>
  {% endfor %}
  </branches>
  <disableSubmodules>false</disableSubmodules>
  <recursiveSubmodules>false</recursiveSubmodules>
  <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
  <authorOrCommitter>false</authorOrCommitter>
  <clean>{{ scm.clean or 'false' }}</clean>
  <wipeOutWorkspace>{{ scm.wipe or 'false' }}</wipeOutWorkspace>
  <pruneBranches>{{ scm.prune or 'false' }}</pruneBranches>
  <remotePoll>false</remotePoll>
  <ignoreNotifyCommit>{{ scm.ignore_notify or 'false' }}</ignoreNotifyCommit>
  <useShallowClone>false</useShallowClone>
  <buildChooser class="hudson.plugins.git.util.DefaultBuildChooser"/>
  <gitTool>Default</gitTool>
  <submoduleCfg class="list"/>
  <relativeTargetDir></relativeTargetDir>
  <reference></reference>
  <excludedRegions>{{ scm.excludedRegions or '' }}</excludedRegions>
  <excludedUsers>{{ scm.excludedUsers or '' }}</excludedUsers>
  <gitConfigName></gitConfigName>
  <gitConfigEmail></gitConfigEmail>
  <skipTag>{{ scm.skip_internal_tag or 'true' }}</skipTag>
  <includedRegions>{{ scm.includedRegions or '' }}</includedRegions>
  <scmName></scmName>
</scm>