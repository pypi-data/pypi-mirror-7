<jenkins.plugins.slack.SlackNotifier_-SlackJobProperty>
  <room>{{ property.room or "#jenkins" }}</room>
  <startNotification>{{ property.msg_start or "true" }}</startNotification>
  <notifySuccess>{{ property.msg_success or "false" }}</notifySuccess>
  <notifyAborted>{{ property.msg_aborted or "false" }}</notifyAborted>
  <notifyNotBuilt>{{property.msg_not_built or "false" }}</notifyNotBuilt>
  <notifyUnstable>{{ property.msg_unstable or "false" }}</notifyUnstable>
  <notifyFailure>{{ property.msg_fail or "false" }}</notifyFailure>
  <notifyBackToNormal>{{ property.msg_back_to_normal or "false" }}</notifyBackToNormal>
</jenkins.plugins.slack.SlackNotifier_-SlackJobProperty>