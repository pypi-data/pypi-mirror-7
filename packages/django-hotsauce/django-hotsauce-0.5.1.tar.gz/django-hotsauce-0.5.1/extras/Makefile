# Makefile here should take care of dependencies so they can be installed
# automatically
MODULES =? 

ifdef WITH_SCHEVO:
 MODULES += libdurus libschevo libxdserver
endif
ifdef WITH_LIBAUTHKIT:
 MODULES += libauthkit
endif 
ifdef WITH_MOINMOIN:
 MODULES += wikiapp
endif
ifdef WITH_BLOGENGINE:
 MODULES += blogengine
endif 
