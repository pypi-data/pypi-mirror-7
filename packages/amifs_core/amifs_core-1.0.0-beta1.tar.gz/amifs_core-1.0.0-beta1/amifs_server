import sys, amifs.fs

#############################################################################

class FSExample(amifs.fs.FS):
	#####################################################################

	def __init__(self, *args, **kw):
		super(FSExample, self).__init__('nedm', *args, **kw)

#############################################################################

if __name__ == '__main__':
	server = FSExample()

	server.multithreaded = False

	server.parse()
	server.main()

	sys.exit(0)

#############################################################################
