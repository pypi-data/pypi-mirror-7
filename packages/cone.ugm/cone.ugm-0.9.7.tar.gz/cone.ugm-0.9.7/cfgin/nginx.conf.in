events {
    worker_connections  1024;
}
http {
    include mime.types;
    default_type application/octet-stream;
    sendfile on;
    keepalive_timeout 65;
    server {
        listen localhost:${:port} default;
        location / {
            proxy_pass http://localhost:8081/;
            include ${buildout:directory}/etc/proxy.conf;
        }
    }
}