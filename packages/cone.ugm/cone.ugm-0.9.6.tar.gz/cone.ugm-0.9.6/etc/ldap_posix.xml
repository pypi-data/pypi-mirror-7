<properties>
  <uri>ldap://127.0.0.1:12345</uri>
  <user>cn=Manager,dc=my-domain,dc=com</user>
  <password>secret</password>
  <cache>0</cache>
  <users_dn>ou=users,ou=posixGroups,dc=my-domain,dc=com</users_dn>
  <users_scope>1</users_scope>
  <users_query></users_query>
  <users_object_classes>
    <item>top</item>
    <item>inetOrgPerson</item>
    <item>posixAccount</item>
    <item>shadowAccount</item>
  </users_object_classes>
  <users_aliases_attrmap>
    <elem>
      <key>rdn</key>
      <value>uid</value>
    </elem>
    <elem>
      <key>id</key>
      <value>uid</value>
    </elem>
    <elem>
      <key>login</key>
      <value>uid</value>
    </elem>
  </users_aliases_attrmap>
  <users_form_attrmap>
    <elem>
      <key>id</key>
      <value>Id</value>
    </elem>
    <elem>
      <key>userPassword</key>
      <value>Password</value>
    </elem>
    <elem>
      <key>cn</key>
      <value>Fullname</value>
    </elem>
    <elem>
      <key>sn</key>
      <value>Surname</value>
    </elem>
  </users_form_attrmap>
  <users_listing_columns>
    <elem>
      <key>cn</key>
      <value>Fullname</value>
    </elem>
  </users_listing_columns>
  <users_listing_default_column>cn</users_listing_default_column>
  <users_exposed_attributes></users_exposed_attributes>
  <users_account_expiration>True</users_account_expiration>
  <users_expires_attr>shadowExpire</users_expires_attr>
  <users_expires_unit>0</users_expires_unit>
  <user_id_autoincrement>True</user_id_autoincrement>
  <user_id_autoincrement_prefix>uid</user_id_autoincrement_prefix>
  <user_id_autoincrement_start>0</user_id_autoincrement_start>
  <users_portrait>True</users_portrait>
  <users_portrait_attr>jpegPhoto</users_portrait_attr>
  <users_portrait_accept>image/jpeg</users_portrait_accept>
  <users_portrait_width>85</users_portrait_width>
  <users_portrait_height>108</users_portrait_height>
  <users_local_management_enabled>True</users_local_management_enabled>
  <groups_dn>ou=groups,ou=posixGroups,dc=my-domain,dc=com</groups_dn>
  <groups_scope>1</groups_scope>
  <groups_query></groups_query>
  <groups_object_classes>
    <item>posixGroup</item>
  </groups_object_classes>
  <groups_aliases_attrmap>
    <elem>
      <key>rdn</key>
      <value>cn</value>
    </elem>
    <elem>
      <key>id</key>
      <value>cn</value>
    </elem>
  </groups_aliases_attrmap>
  <groups_form_attrmap>
    <elem>
      <key>id</key>
      <value>Id</value>
    </elem>
  </groups_form_attrmap>
  <groups_relation></groups_relation>
  <groups_listing_columns>
    <elem>
      <key>id</key>
      <value>Groupname</value>
    </elem>
  </groups_listing_columns>
  <groups_listing_default_column>id</groups_listing_default_column>
  <roles_dn>ou=roles,ou=posixGroups,dc=my-domain,dc=com</roles_dn>
  <roles_scope>1</roles_scope>
  <roles_query></roles_query>
  <roles_object_classes>
    <item>groupOfUniqueNames</item>
  </roles_object_classes>
  <roles_aliases_attrmap>
    <elem>
      <key>rdn</key>
      <value>cn</value>
    </elem>
    <elem>
      <key>id</key>
      <value>cn</value>
    </elem>
  </roles_aliases_attrmap>
  <roles_form_attrmap>
    <elem>
      <key>id</key>
      <value>Id</value>
    </elem>
  </roles_form_attrmap>
  <roles_relation></roles_relation>
  <default_membership_assignment_widget>inout</default_membership_assignment_widget>
  <user_display_name_attr>cn</user_display_name_attr>
  <group_display_name_attr>id</group_display_name_attr>
</properties>
