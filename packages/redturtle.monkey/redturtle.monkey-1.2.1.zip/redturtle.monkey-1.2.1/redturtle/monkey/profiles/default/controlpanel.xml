<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="redturtle.monkey"
    purge="False">

    <configlet
        title="Monkey"
        action_id="monkey"
        appId="redturtle.monkey"
        category="Products"
        condition_expr=""
        icon_expr="string:${portal_url}/++resource++redturtle.monkey.images/monkey.gif"
        url_expr="string:${portal_url}/@@monkey-settings"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
