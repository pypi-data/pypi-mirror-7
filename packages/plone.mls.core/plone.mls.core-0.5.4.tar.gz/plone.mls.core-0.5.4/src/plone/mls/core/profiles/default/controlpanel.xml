<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="plone.mls.core"
    purge="False">

  <configlet
      action_id="propertyshelf_mls"
      appId="plone.mls.core"
