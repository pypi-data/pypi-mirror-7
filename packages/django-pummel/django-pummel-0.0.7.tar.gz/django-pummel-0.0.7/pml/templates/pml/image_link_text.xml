{% extends 'pml/base_image_container.xml' %}
{% load pml_inclusion_tags %}

{% block tags %}
    {% image_tag image_url 'true' 'left' %}
    <TITLE>
        <LINK href="{{ link_url }}">
            {{ link_text }}
        </LINK>
    </TITLE>
    <TEXT>
        {{ body_text }}
    </TEXT>
{% endblock %}
