msgid "Warning"
msgstr "Warnung"

msgctxt "error:account.account.open_aged_balance:0"
msgid "You cannot define overlapping terms"
msgstr "Die eingegebenen Zeiträume dürfen sich nicht überschneiden."

msgctxt "error:account.account.template:0"
msgid "You can not create recursive accounts!"
msgstr "Konten können nicht rekursiv angelegt werden!"

