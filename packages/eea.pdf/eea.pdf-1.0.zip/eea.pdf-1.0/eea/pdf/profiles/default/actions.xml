<?xml version="1.0"?>
<object name="portal_actions">
 <action-provider name="portal_actions"/>
 <object name="document_actions">
  <object name="send_as_pdf">
   <property name="icon_expr">string:$portal_url/++resource++eea.pdf.images/email-pdf.png</property>
   <property name="permissions">
    <element value="Modify portal content"/>
   </property>
   <property name="visible">False</property>
  </object>
  <object name="download_as_pdf">
   <property name="icon_expr">string:$portal_url/++resource++eea.pdf.images/download-pdf.png</property>
   <property name="url_expr">string:$object_url/download.pdf</property>
   <property name="permissions">
    <element value="Modify portal content"/>
   </property>
  </object>
 </object>
</object>
