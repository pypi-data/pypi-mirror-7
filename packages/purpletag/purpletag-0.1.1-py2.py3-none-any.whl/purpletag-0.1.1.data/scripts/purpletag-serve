#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'purpletag==0.1.1','console_scripts','purpletag-serve'
__requires__ = 'purpletag==0.1.1'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('purpletag==0.1.1', 'console_scripts', 'purpletag-serve')()
    )
