<?xml version="1.0"?>
<object name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="interlegis.portalmodelo.pl"
    purge="False">
    <configlet title="SAPL Sync Settings"
        action_id="sapl-sync"
        appId="interlegis.portalmodelo.pl"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@sapl-sync-settings"
        icon_expr="string:${portal_url}/++resource++interlegis.portalmodelo.pl/interlegis_icon.png"
        visible="True"
        i18n:attributes="title">
        <permission>Manage portal</permission>
    </configlet>
</object>
