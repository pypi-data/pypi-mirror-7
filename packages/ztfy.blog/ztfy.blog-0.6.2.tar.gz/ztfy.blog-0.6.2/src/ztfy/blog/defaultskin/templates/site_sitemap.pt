<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
		xmlns:tal="http://xml.zope.org/namespaces/tal">
	<url tal:repeat="content view/getContents">
		<loc tal:content="content/@@absolute_url" />
		<lastmod tal:define="modified content/zope:modified" tal:content="python:modified.strftime('%Y-%m-%dT%H:%M:%S+00:00')" />
	</url>
</urlset>