<div class="prefix">
	<div class="langs" tal:define="langs view/langs" tal:condition="python:len(langs) > 1">
		<ul>
			<li tal:repeat="lang langs">
				<a tal:attributes="href string:@@setLanguage.html?language=${lang}"><img tal:attributes="src string:/--static--/ztfy.i18n/img/flags/${lang}.png;" alt="" /></a>
			</li>
		</ul>
	</div>
	<tal:if define="image view/banner" condition="image">
		<img tal:define="display image/++display++h90"
			 tal:attributes="src display/@@absolute_url" alt="" />
	</tal:if>
	<div class="clearer"><!-- IE bugfix --></div>
</div>