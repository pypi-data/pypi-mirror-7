<div class="suffix" i18n:domain="ztfy.blog">
	<div class="footer">
		<a href="http://www.python.org" title="Powered by Python" rel="external" i18n:attributes="title">
			<img src="/--static--/ztfy.blog.defaultskin/img/logo_python.png" alt="Python" i18n:attributes="title" />
		</a>
		<a href="http://wiki.zope.org/zope3" title="Powered by Zope3" rel="external" i18n:attributes="title">
			<img src="/--static--/ztfy.blog.defaultskin/img/logo_zope.png" alt="Zope" i18n:attributes="title" />
		</a>
		<a tal:attributes="href string:http://validator.w3.org/check?verbose=1&uri=${context/@@absolute_url}" title="XHTML 1.0 compliant web site" rel="external" i18n:attributes="title">
			<img src="/--static--/ztfy.blog.defaultskin/img/logo_w3c_xhtml.png" alt="Python" i18n:attributes="title" />
		</a>
		<a tal:attributes="href string:http://jigsaw.w3.org/css-validator/validator?profile=css2&warning=2&uri=${context/@@absolute_url}" title="CSS compliant web site" rel="external" i18n:attributes="title">
			<img src="/--static--/ztfy.blog.defaultskin/img/logo_w3c_css.png" alt="Python" i18n:attributes="title" />
		</a>
	</div>
	<div class="credits" tal:define="presentation view/presentation">
		<span i18n:translate="">&copy;<a
			   tal:define="mailto presentation/owner_mailto"
			   tal:omit-tag="not:mailto"
			   tal:attributes="href string:mailto:${mailto}"
			   tal:content="presentation/owner" i18n:name="owner">Thierry Florac</a> <tal:var replace="view/year" i18n:name="year" /></span> -
		<span tal:content="presentation/i18n:signature">All contents available under <a href="http://www.gnu.org/licenses/fdl.html">GNU/FDL licence</a></span>
	</div>
</div>
