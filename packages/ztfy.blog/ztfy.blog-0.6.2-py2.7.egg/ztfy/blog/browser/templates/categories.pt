<div class="form" i18n:domain="ztfy.blog">
	<fieldset>
		<legend tal:condition="view/legend | nothing" tal:content="view/legend" i18n:translate="" />
		<tal:if condition="view/rows">
			<tal:var content="structure view/renderTable" />
			<div class="batch"><div class="batchPages" tal:content="structure view/renderBatch" /></div>
		</tal:if>
		<div tal:condition="not:view/rows">
			<div class="help" tal:content="view/empty_value" i18n:translate="">This container is empty.</div>
		</div>
	</fieldset>
</div>