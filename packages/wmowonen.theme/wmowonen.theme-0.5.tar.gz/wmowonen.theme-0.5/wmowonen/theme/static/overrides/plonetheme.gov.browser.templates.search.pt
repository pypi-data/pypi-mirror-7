<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="plone">

<head>
  <metal:block metal:fill-slot="head_slot">

    <link rel="alternate" title="RSS 1.0" type="application/rss+xml"
          tal:define="here_url context/@@plone_context_state/object_url;"
          tal:condition="request/SearchableText|nothing"
          tal:attributes="href string:$here_url/search_rss?${request/QUERY_STRING}"/>

    <meta name="navigation_root_url" tal:attributes="content view/navroot_url" />

  </metal:block>

  <metal:block fill-slot="top_slot"
               tal:define="dummy python:request.set('disable_border',1);
                           disable_column_one python:request.set('disable_plone.leftcolumn',1);
                           disable_column_two python:request.set('disable_plone.rightcolumn',1);" />
</head>

<body>

  <div id="content-core" metal:fill-slot="main">
      
      <div tal:define="portal context/@@plone_portal_state/portal;
                      first_call not:request/advanced_search|nothing;
                      st python:request.get('SearchableText', '');
                      DateTime python:modules['DateTime'].DateTime;
                      navigation_root_url view/navroot_url;
		      searchterm request/SearchableText|nothing;
		      " omit-tag="">
    <form name="searchform"
          action="@@search"
          class="searchPage enableAutoFocus"
          > 

        <input type="hidden" name="advanced_search" value="False" />
        <input type="hidden" name="sort_on" value="" />

        <!-- fieldset id="search-field" i18n:domain="plone" class="noborder" -->
	<div class="input-group">
	    <input type="search" name="SearchableText" class="form-control input-lg" placeholder="Zoeken"
		   i18n:attributes="title title_search_title;"
                   tal:attributes="value st;"/><span class="input-group-btn">
	    <button class="btn btn-primary btn-lg allowMultiSubmit" role="submit" type="submit">
                    Zoeken
                  </button>
	    </span>
	</div>
        <!-- /fieldset -->
    </form>
		<div class="harcodedResultTypes" tal:define="urlBase python:searchterm != '' and searchterm is not None and here.absolute_url() + '/@@search?SearchableText=' + searchterm or here.absolute_url();
						     fullURL python:here.absolute_url() + '/@@search?' +  request['QUERY_STRING'];">   
		<ul>
			<li tal:define="thisURL python:urlBase">
			      <a href="" tal:attributes="href thisURL;
						         class python: thisURL == fullURL and 'selected';">
				    Alles
			      </a>
			</li>
			<li tal:define="thisURL python:urlBase + '&path=%2F' + portal.id + '%2Fnieuws'">
			      <a href="" tal:attributes="href thisURL;
						         class python: thisURL == fullURL and 'selected';">
				    Nieuws
			      </a>
			</li>
			<li tal:define="thisURL python:urlBase + '&path=%2F' + portal.id + '%2Fregios'">
			      <a href="" tal:attributes="href thisURL;
						         class python: thisURL == fullURL and 'selected';">
			            Regio's
			      </a>
			</li>
			<li tal:define="thisURL python:urlBase + '&path=%2F' + portal.id + '%2Fprojecten'">
			      <a href="" tal:attributes="href thisURL;
						         class python: thisURL == fullURL and 'selected';">
				    Projecten
			      </a>
			</li>
			<li tal:define="thisURL python:urlBase + '&path=%2F' + portal.id + '%2Fdocumenten'">
			      <a href="" tal:attributes="href thisURL;
						         class python: thisURL == fullURL and 'selected';">
				    Publicaties
			      </a>
			</li>
			<li tal:define="thisURL python:urlBase + '&path=%2F' + portal.id + '%2Fuitgelicht'">
			      <a href="" tal:attributes="href thisURL;
						         class python: thisURL == fullURL and 'selected';">
			            Uitgelicht
			      </a>
			</li>
		</ul>	
	</div>

      <div class="harcodedThemas" tal:define="urlBase python:searchterm != '' and searchterm is not None and here.absolute_url() + '/@@search?SearchableText=' + searchterm or here.absolute_url();
						     fullURL python:here.absolute_url() + '/@@search?' +  request['QUERY_STRING'];">   
		<ul>
			<li tal:define="thisURL python:urlBase + '&path=%2F' + portal.id + '%2Fthemas%2Fnieuwe-wmo'">
			      <a href="" tal:attributes="href thisURL;
						         class python: thisURL == fullURL and 'selected';">
				    Nieuwe Wmo
			      </a>
			</li>
			<li tal:define="thisURL python:urlBase + '&path=%2F' + portal.id + '%2Fthemas%2Flokale-kracht'">
			      <a href="" tal:attributes="href thisURL;
						         class python: thisURL == fullURL and 'selected';">
			            Lokale kracht
			      </a>
			</li>
			<li tal:define="thisURL python:urlBase + '&path=%2F' + portal.id + '%2Fthemas%2Fwonen-en-zorg'">
			      <a href="" tal:attributes="href thisURL;
						         class python: thisURL == fullURL and 'selected';">
				    Wonen en zorg
			      </a>
			</li>
			<li tal:define="thisURL python:urlBase + '&path=%2F' + portal.id + '%2Fthemas%2Fsociale-wijkteams'">
			      <a href="" tal:attributes="href thisURL;
						         class python: thisURL == fullURL and 'selected';">
				    Sociale Wijkteams
			      </a>
			</li>
			<li tal:define="thisURL python:urlBase + '&path=%2F' + portal.id + '%2Fthemas%2Fkleine-kernen'">
			      <a href="" tal:attributes="href thisURL;
						         class python: thisURL == fullURL and 'selected';">
			            Kleine kernen
			      </a>
			</li>
		</ul>	
	</div>
	
        <div class="row">

            <div class="visualClear"><!-- --></div>
            <div id="search-results-wrapper"
                 class="cell width-full position-0"
                 tal:define="b_start python:0;
                             b_start request/b_start | b_start;
                             batch python: view.results(b_start=b_start);
                             normalizeString nocall:context/@@plone/normalizeString;">

                <div class="visualClear"><!-- --></div>

                <metal:searchresults define-macro="search_results">
                    <div id="search-results"
                         tal:define="navigation_root_url context/@@plone_portal_state/navigation_root_url;">

                        <metal:noresults tal:condition="not: batch">
                          <p i18n:domain="plone"><strong i18n:translate="description_no_results_found">No results were found.</strong></p>
                        </metal:noresults>

                        <metal:results tal:condition="batch"
                                       tal:define="isAnon context/@@plone_portal_state/anonymous;
                                                   toLocalizedTime nocall: context/@@plone/toLocalizedTime;
                                                   site_properties context/portal_properties/site_properties;
                                                   use_view_action site_properties/typesUseViewActionInListings|python:();
                                                   allowAnonymousViewAbout site_properties/allowAnonymousViewAbout;
                                                   show_about python:not isAnon or allowAnonymousViewAbout;">
			      <dl class="searchResults">
        <tal:results repeat="result batch">
	    <tal:entry define="result_url result/getURL;
			    plone_view context/@@plone;
			    use_types_blacklist request/use_types_blacklist | python:True;
			    use_navigation_root request/use_navigation_root | python:True;
			    here_url context/@@plone_context_state/object_url;
			    normalizeString nocall:context/@@plone/normalizeString;
			     results python:here.queryCatalog(REQUEST=request,use_types_blacklist=use_types_blacklist, use_navigation_root=use_navigation_root);
			     Batch python:modules['Products.CMFPlone'].Batch;
			     b_size python:30;b_start python:0;b_start request/b_start | b_start;
			     searchterm request/SearchableText|nothing;
			     use_view_action python:();
			     searchview context/@@gov_searchview;
			     toLocalizedTime nocall:context/@@plone/toLocalizedTime;
			     portal context/@@plone_portal_state/portal;
			       result_type result/portal_type;
			       item_icon python:plone_view.getIcon(result);
			       url python:'%s?searchterm=%s'%(searchview.test(result_type in use_view_action, result_url+'/view', result_url+'/'), searchterm);">
	       
		  <div class="search_result_box">
		  <dt tal:define="item_wf_state_class string:state-${result/review_state};"
                tal:attributes="class python:'contenttype-' + normalizeString(result.portal_type)">



	      <a
                 href="#"
                 tal:attributes="href url;
                                 class string:$item_wf_state_class resultHeading"
	         tal:condition="python: result.portal_type != 'File'"
                 tal:content="python:result.getObject().pretty_title_or_id()" />

	      <a
                 href="#"
                 tal:attributes="href url;
                                 class string:$item_wf_state_class resultHeading"
	         tal:condition="python: result.portal_type == 'File'"
                 tal:content="python: searchview.trim_description(result.getObject().pretty_title_or_id(), 30)" />

            </dt>

            <dd>
            
      
              <div class="thema-tag"
                  tal:define="
                      tagInfo python: context.restrictedTraverse('virtualTreeTag').tag(result.Subject());
                      "
                  tal:condition="tagInfo">
                <span tal:define="
                      tagId tagInfo/id;
                      tagTitle tagInfo/title;
                      "
                    tal:attributes="class string:tag-${tagId}"
                    tal:content="tagTitle">
                  Tag
                </span>
              </div>
              
              
		  <tal:eventDate condition="python:result.getObject().portal_type == 'Event'">

		        <span tal:define="future python:result.getObject().end() > DateTime();
			      pec string:pastEventTable;
			      fec string:futureEventTable;
			      eventclass python:future and fec or pec" tal:omit-tag="">
			<tal:differentday tal:condition="python:not result.getObject().start().Date() == result.getObject().end().Date()">
				<div class="" tal:attributes="class python:future and 'futureEventDateFrom' or 'pastEventDateFrom'">
					<div class="eventDayInfo" tal:content="python:result.getObject().start().day()">02</div>
					<div class="eventMonthInfo" tal:content="python:result.getObject().start().strftime('%b').capitalize()">Jun</div>
				        <div class="eventYearInfo" tal:content="python:result.getObject().start().strftime('%Y').capitalize()">2010</div>
				</div>
				<div class="" tal:attributes="class python:future and 'futureEventDateUntil' or 'pastEventDateUntil'">
					<div class="eventDayInfo" tal:content="python:result.getObject().end().day()">02</div>
					<div class="eventMonthInfo" tal:content="python:result.getObject().end().strftime('%b').capitalize()">Jun</div>
				        <div class="eventYearInfo" tal:content="python:result.getObject().end().strftime('%Y').capitalize()">2010</div>
				</div>
                         </tal:differentday>

                        <tal:sameday tal:condition="python:result.getObject().start().Date() == result.getObject().end().Date()"
                                     tal:define="ampm python:site_properties.getProperty('localLongTimeFormat').find('%p') >= 0">
				<div class="" tal:attributes="class python:future and 'futureEventDateFrom' or 'pastEventDateFrom'">
                                        <div class="eventDayInfo" tal:content="python:result.getObject().start().day()">02</div>
                                        <div class="eventMonthInfo" tal:content="python:result.getObject().start().strftime('%b').capitalize()">Jun</div>
				        <div class="eventYearInfo" tal:content="python:result.getObject().start().strftime('%Y').capitalize()">2010</div>
				</div>
                        </tal:sameday>
			</span>
		  </tal:eventDate>

		  <div class="result-publish-date discreet">Gepubliceerd op <span tal:replace="python:toLocalizedTime(result.Date(), long_format=0)">02-06-2011</span></div>


      <div class="descriptionWrapper">
        <p class="description" tal:content="result/Description">
          Description
        </p>
      </div>

                <!-- span tal:replace="python:plone_view.cropText(result.Description, desc_length, desc_ellipsis)">
                    Cropped description
                </span -->

            <span class="documentByLine" tal:condition="nothing">
                <span class="documentAuthor"
                    i18n:translate="label_by_author">
                by
                <a href="#"
                   tal:attributes="href string:${portal_url}/author/${result/Creator}"
                   tal:content="result/Creator"
                   tal:omit-tag="not:result/Creator"
                   i18n:name="author">
                Bob Dobalina
                </a>

                </span>

                <span class="documentModified">
                  &mdash;
                    <span i18n:translate="box_last_modified">
                    last modified
                    </span>
                    <span tal:replace="python:toLocalizedTime(result.ModificationDate, long_format=1)">
                    August 16, 2001 at 23:35:59
                    </span>
                </span>


                <span tal:define="categories result/Subject|nothing"
                      tal:condition="categories">
                    &mdash;
                    <tal:filedunder i18n:translate="label_filed_under">filed under:</tal:filedunder>
                        <span tal:repeat="category categories">
                            <a href=""
                               class="link-category"
                               rel="tag"
                               tal:content="category"
                               tal:attributes="href string:${portal_url}/search?Subject%3Alist=${category}">
                               Category
                            </a><tal:separator condition="not: repeat/category/end">,</tal:separator>
                        </span>
                </span>

                <span class="relevance"
                      tal:condition="result/data_record_normalized_score_">
                    &mdash;
                    <tal:relevance i18n:translate="label_relevance_percentage">
                    Relevance:
                        <span tal:content="result/data_record_normalized_score_"
                               tal:omit-tag=""
                               i18n:name="percentage">23</span>%
                    </tal:relevance>
                </span>
            </span>

            </dd>
		</div>
	    </tal:entry>
	    </tal:results>
	    </dl>
                        </metal:results>
                    </div>
                </metal:searchresults>

            </div>
        </div>

        <div class="visualClear"><!-- --></div>
    </div>

  </div>

</body>
</html>
